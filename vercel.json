{
    "functions": {
        "api/app.php": {
            "runtime": "vercel-php@0.7.1"
        }
    },
    "routes": [
        {
            "'src": "api/index.php",
            "dest": "api/index.php"
        },
        {
            "src": "/styles.css",
            "dest": "public/styles.css"
        },
        {
            "src":"/(.*)",
            "dest":"api/index.php"
        }
    ]
}